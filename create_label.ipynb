    "import os\n",
    "import pandas as pd\n",
    "\n",
    "os.chdir(r'dataset/training')\n",
    "\n",
    "folders = ['cat', 'bird']\n",
    "\n",
    "files = []\n",
    "\n",
    "for folder in folders:\n",
    "    for file in os.listdir(folder):\n",
    "        files.append([file, folder])\n",
    "\n",
    "pd.DataFrame(files, columns=['files', 'target']).to_csv('training_label.csv')"
   ]
  }
